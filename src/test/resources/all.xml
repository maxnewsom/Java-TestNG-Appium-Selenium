<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Appium and Selenium Suite" verbose="1" parallel="tests" >

    <listeners>
        <listener class-name="com.saucelabs.yy.Tests.TestListener"/>
    </listeners>

    <test name="Appium" parallel="methods">
        <packages>
            <package name="com.saucelabs.yy.Tests.Appium"/>
        </packages>
    </test>

    <test name="Selenium" parallel="methods">
        <packages>
            <package name="com.saucelabs.yy.Tests.Selenium"/>
        </packages>
    </test>
</suite>